#include <stdio.h>
#include <omp.h>

int main()
{
    #pragma omp parallel sections
    {
        #pragma omp section
        {
            for (int i = 1; i <= 5; i++) printf("2 ");
        }
        #pragma omp section
        {
            for (int i = 1; i <= 5; i++) printf("4 ");
        }
    }
    return 0;
}
